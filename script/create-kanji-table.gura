#!/usr/bin/env gurax
[(1, 8), (16, 47), (48, 84)].Each {|pair|
	[rowStart, rowEnd] = pair
	repeat (rowEnd - rowStart + 1) {|i|
		high = i + 0x20 + rowStart
		repeat (94) {|j|
			low = j + 0x20
			codeJIS = (high << 8) + low
			codeUTF32 = codecs.japanese.ConvJISToUTF32(codeJIS)
			if (codeUTF32 == 0) {
				Printf('  ')
			} else {
				Printf('%C', codeUTF32)
			}
		}
		Println()
	}
}
